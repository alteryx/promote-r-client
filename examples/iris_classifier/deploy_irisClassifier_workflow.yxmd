      <Properties>
        <Configuration>
          <RScript><![CDATA[df <- read.Alteryx("#1", mode="data.frame")
df2 <- read.Alteryx("#2", mode="data.frame")

# add our libraries
library(randomForest)
library(promote)

# train our model
Rf_fit<-randomForest(formula=species~., data=df)

# create our prediction function
model.predict <- function(data) {
  pred <- predict(Rf_fit, newdata=data.frame(data))
  result <- data.frame(prediction=pred)
  result 
}

# create some test data and test our model locally
testdata <- df[2,1:4]
model.predict(testdata)
# sample JSON for prediction
#{"sepal_length":5.1,"sepal_width":3.5,"petal_length":1.4,"petal_width":0.2}

# Promote configuration
promote.library("randomForest")

promote.config <- c(
  username=toString(df2$USERNAME),
  apikey=toString(df2$APIKEY),
  env=toString(df2$PROMOTE_URL)
)

# deployment
promote.deploy("IrisClassifierDesigner", confirm = FALSE)
]]></RScript>
